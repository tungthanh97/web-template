{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^14 || ^16"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.39",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "formik": "^2.2.9",
    "http-proxy-middleware": "^2.0.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-error-boundary": "^3.1.4",
    "react-redux": "^7.1.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --bail --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx  src",
    "lintfix": "eslint --ext .js,.jsx,.ts,.tsx  src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "format:lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,scss}\""
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/eslint-parser": "^7.11.3",
    "@babel/helper-builder-react-jsx": "^7.12.13",
    "@babel/helper-regex": "^7.10.5",
    "@babel/parser": "^7.13.10",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/traverse": "^7.13.0",
    "@babel/types": "^7.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^13.1.8",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/react-helmet": "^6.1.5",
    "@types/react-input-mask": "^3.0.1",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/recharts": "^1.8.20",
    "@types/redux-mock-store": "^1.0.2",
    "@types/sinon": "^10.0.6",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "axios-mock-adapter": "^1.20.0",
    "babel-plugin-styled-components": "^2.0.7",
    "cross-env": "^7.0.3",
    "enzyme": "^3.10.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^2.6.2",
    "redux-devtools": "^3.5.0",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^27.0.7",
    "type-coverage": "^2.19.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-class-properties",
      [
        "babel-plugin-styled-components",
        {
          "displayName": true
        }
      ]
    ]
  },
  "jest": {
    "resetMocks": false,
    "transformIgnorePatterns": [
      "node_modules/(?!d3-shape|d3-path|d3-scale|d3-array|internmap|d3-interpolate|d3-color|d3-format|d3-time)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 52.1,
        "functions": 52.7,
        "lines": 59.4,
        "statements": 59.2
      }
    },
    "coverageReporters": [
      "lcov"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,tsx,ts}": [
      "eslint --fix",
      "prettier --write",
      "git add",
      "cross-env CI=true react-scripts test --env=jsdom --findRelatedTests"
    ]
  }
}
